{
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": ["eslint:recommended"],
  "rules": {
    "quotes": ["error", "single"]
  },
  "globals": {
    "$": true
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error"
  },
  "prettier/prettier": [
    "error",
    {
      "tabWidth": 2,
      "singleQuote": false
    }
  ]
}
